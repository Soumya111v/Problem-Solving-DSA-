#include<bits/stdc++.h>
using namespace std;

int main()
{
  long int n,k;
  cin>>n>>k;
  long int arr[n];
  for(long int i=0;i<n;i++){
      cin>>arr[i];
  }
  sort(arr,arr+n);
  long int min = arr[k-1];
  long int max=arr[n-k];
  cout<<"The minimum Kth element is : "<<min<<"\n";
  cout<<"The maximum kth element is: "<<max<<"\n";
  return 0;
}
