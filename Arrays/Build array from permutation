#include <bits/stdc++.h>
using namespace std;
int main()
{
    int m;
    cin>>m;
   vector<int> nums;
   for(int i=0;i<m;i++){
       int n;
       cin>>n;
       nums.push_back(n);
   }
      vector<int>v3;
        for(int i=0;i<nums.size();i++){
            v3.push_back(nums[nums[i]]);
        }
        for(auto it=v3.begin();it!=v3.end();it++){
            cout<< *it;
        }

    return 0;
}
